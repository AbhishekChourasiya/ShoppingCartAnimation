<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.learn.cart">


    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 震动 -->
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.github.learn.cart.ui.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.taobao.AddToCartOfTaoBaoActivity"
            android:label="@string/shopping_cart_of_tao_bao"/>
        <activity android:name=".ui.jd.AddToCartOfJDActivity"
                  android:label="@string/shopping_cart_of_jd"/>
        <activity android:name=".ui.amazon.AddToCartOfAmazonActivity"
                  android:label="@string/shopping_cart_of_amazon"/>
        <activity android:name=".ui.simple.AddToCartOfSimpleActivity"
                  android:label="@string/shopping_cart_of_simple"/>
    </application>

</manifest>
